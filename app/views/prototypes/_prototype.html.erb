<div class="card">
  <%= link_to image_tag(prototype.image, class: :card__img ), prototype_path(prototype) %>
  <%# link_to image_tag prototype.image, class: 'prototype-image' "/users/#{current_user.id}", method: :get, class: :card__title%> <%# if message.image.attached? %>
  <div class="card__body">
     
    <%= link_to prototype.title, "/users/#{current_user.id}", method: :get, class: :card__title %>
    <p class="card__summary">
      <%= prototype.catch_copy %>
    </p>
    <%= link_to prototype.user.name, "/users/#{current_user.id}", method: :get, class: :card__user %>
  </div>
</div>